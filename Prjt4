#include<stdio.h>
int main()
{
	int n,m,i,j ;
	int mat1[100][100],mat2[100][100],mat3[100][100];
	do 
	{
		printf("donner n:");
		scanf("%d",&n);
		printf("donner m:");
		scanf("%d",&m);
		
	}while ((n<1) && (m<1));
	
	for (i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
			printf("mat1[%d][%d] :",i,j);
			scanf("%d",&mat1[i][j]);
		}
	}
	for (i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
			printf("mat2[%d][%d] :",i,j);
			scanf("%d",&mat2[i][j]);
		}
	}
	for (i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
			mat3[i][j]=mat1[i][j]+mat2[i][j];
		}
	}
	for (i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
			printf("  %d",mat3[i][j]);
		}
		printf("\n");
	}
			
}
